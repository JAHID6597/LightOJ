#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int n,i,c;
        cin>>n;
        if(n<=10)
        {
            cout<<n<<" "<<0<<endl;
        }
        else{
            c=n-10;
            cout<<10<<" "<<c<<endl;
        }
    }
    return 0;
}
